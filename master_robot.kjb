    <entry>
      <name>Execucao Bloqueada</name>
      <description/>
      <type>EVAL_TABLE_CONTENT</type>
      <attributes/>
      <connection>DIRO</connection>
      <schemaname>robot</schemaname>
      <tablename>registro</tablename>
      <success_condition>rows_count_greater</success_condition>
      <limit>0</limit>
      <is_custom_sql>Y</is_custom_sql>
      <is_usevars>Y</is_usevars>
      <custom_sql>select 
 nvl(ultima_execucao.id_execucao,0) as ultimo_id_execucao
from 
 (select 
   max(id_execucao) as id_execucao
  from
   ${NOME_SCHEMA_DIRO}.execucao
  where
   ip_computador       = '${var_ip_computador}'
   and nome_computador = '${var_nome_computador}') ultima_execucao 
  left join ${NOME_SCHEMA_DIRO}.execucao
    on execucao.id_execucao = ultima_execucao.id_execucao
where
 exists (select 1
         from ${NOME_SCHEMA_DIRO}.registro
         where ip_computador      = '${var_ip_computador}'
           and nome_computador    = '${var_nome_computador}'
           and execucao_bloqueada = 0)</custom_sql>
