    <entry>
      <name>SQL - atualiza final execucao</name>
      <description/>
      <type>SQL</type>
      <attributes/>
      <sql>update DIRO.execucao
set data_hora_fim_execucao = null,
    executou_algo = (case when codigo_comportamento > 0 then 0 else 1 end)
where 
 id_execucao = ${var_id_execucao};

/*****************************************************************************
 Atualiza data final da execução das tarefas vinculadas a esta execucao
  -> Condicoes:
  1. Se existir o registro de processamento do artefato em questão situacao  
     valida (V) e com data de processamento preenchida (data_hora_fim)
*/
update ${NOME_SCHEMA_DIRO}.execucao_tarefa
set data_hora_fim_execucao = sysdate
where
 exists (select 1
         from 
          ${NOME_SCHEMA_DIRO}.comportamento_log
         where
          to_char(comportamento_log.data_hora_fim, 'YYYY-MM-DD') = to_char(execucao_tarefa.data_hora_inicio_execucao, 'YYYY-MM-DD')
          and upper(comportamento_log.schema_artefato)           = upper(execucao_tarefa.schema_artefato)
          and upper(comportamento_log.nome_artefato)             = upper(execucao_tarefa.nome_artefato)
          and comportamento_log.situacao                         = 'V'
          and comportamento_log.data_hora_fim                    is not null)
 and data_hora_inicio_execucao = to_timestamp('${var_data_hora_atual}', 'YYYY-MM-DD HH24:MI:SS.FF');

/*****************************************************************************
 Atualiza data final da execucao se todos os artefatos do comportamento 
  vinculados a esta execucao forem procesados. 
  -> Condicoes:
  1. Igual a execução em questao
  2. Exista pelo menos um artefato valido (V) vinculado a esta execucao o
  3. Nao exista nenhum artefato valido (V) com data de processamento 
     (data_hora_fim) vazia 
*/
update ${NOME_SCHEMA_DIRO}.execucao
set data_hora_fim_execucao = sysdate,
    executou_algo = 1,
    execucao_bloqueada = 0
where 
 id_execucao = ${var_id_execucao}
 and exists (select 1
             from ${NOME_SCHEMA_DIRO}.comportamento_log
             where comportamento_log.id_execucao = ${var_id_execucao}
               and comportamento_log.situacao = 'V')
 and not exists (select 1
                 from ${NOME_SCHEMA_DIRO}.comportamento_log
                 where comportamento_log.id_execucao = ${var_id_execucao}
                   and comportamento_log.data_hora_fim is null
                   and comportamento_log.situacao = 'V')
 and not exists (select 1
                 from ${NOME_SCHEMA_DIRO}.execucao_tarefa
                 where execucao_tarefa.codigo_comportamento = ${var_codigo_comportamento}
                   and to_char(execucao_tarefa.data_hora_inicio_execucao, 'YYYY-MM-DD') = to_char(sysdate, 'YYYY-MM-DD')
                   and execucao_tarefa.data_hora_fim_execucao is null);

/*****************************************************************************
 Atualiza data final da execução caso a execucao em questao nao tenha identificado
  artefatos para processar  
  -> Condicoes:
  1. Igual a execução em questão
  2. Nao exista nenhum artefato vinculado a esta execucao
*/
update ${NOME_SCHEMA_DIRO}.execucao
set data_hora_fim_execucao = sysdate
where 
 id_execucao = ${var_id_execucao}
 and not exists (select 1
                 from ${NOME_SCHEMA_DIRO}.comportamento_log
                 where comportamento_log.id_execucao = ${var_id_execucao}
                   and comportamento_log.situacao = 'V');

/*****************************************************************************
 Exclui o registro da execucao da tarefa caso nao exista um registro do 
  comportamento indicado o processamento do artefato
*/
delete from ${NOME_SCHEMA_DIRO}.execucao_tarefa
where
 exists (select 1
         from ${NOME_SCHEMA_DIRO}.comportamento_log
         where to_char(comportamento_log.data_hora_fim, 'YYYY-MM-DD') = to_char(execucao_tarefa.data_hora_inicio_execucao, 'YYYY-MM-DD')
           and upper(comportamento_log.schema_artefato)               = upper(execucao_tarefa.schema_artefato)
           and upper(comportamento_log.nome_artefato)                 = upper(execucao_tarefa.nome_artefato))
 and not exists (select 1
                 from ${NOME_SCHEMA_DIRO}.comportamento_log
                 where to_char(comportamento_log.data_hora_fim, 'YYYY-MM-DD') = to_char(execucao_tarefa.data_hora_inicio_execucao, 'YYYY-MM-DD')
                   and upper(comportamento_log.schema_artefato)               = upper(execucao_tarefa.schema_artefato)
                   and upper(comportamento_log.nome_artefato)                 = upper(execucao_tarefa.nome_artefato)
                   and comportamento_log.situacao                             = 'V');</sql>
