    <connection>DIRO</connection>
    <sql>select
 metadados.nome_artefato,
 metadados.nome_coluna,
 metadados.tipo_coluna,
 metadados_tipos.tipo_coluna_database,
 metadados.tamanho_coluna,
 metadados.decimal_coluna,
 metadados.posicao_coluna 
from 
 ${NOME_SCHEMA_DIRO}.dimensao
  join ${NOME_SCHEMA_DIRO}.metadados
    on upper(metadados.nome_schema)   = upper(dimensao.nome_schema_origem)
   and upper(metadados.nome_artefato) = upper(dimensao.nome_artefato_origem)
   and metadados.posicao_coluna = 1
   and metadados.data_hora_registro >= to_timestamp('${data_hora_atual}', 'YYYY-MM-DD HH24:MI:SS.FF')
  join ${NOME_SCHEMA_DIRO}.metadados_tipos
    on upper(metadados_tipos.nome_engine_banco_dados_destino) = upper(dimensao.nome_engine_banco_dados_destino)
   and upper(metadados_tipos.tipo_coluna_robot)              =  upper(metadados.tipo_coluna)
where
 upper(dimensao.nome_schema_origem)       = upper('${nome_schema_origem}')
 and upper(dimensao.nome_artefato_origem) = upper('${nome_artefato_origem}')
order by
 metadados.posicao_coluna</sql>
