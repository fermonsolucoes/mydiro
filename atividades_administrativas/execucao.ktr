    <sql>insert into ${NOME_SCHEMA_DIRO}.execucao_tarefa 
 (schema_artefato,
  nome_artefato,
  codigo_comportamento,
  ordem_prioridade,
  hora_inicio_execucao)
select 
 nome_schema_destino     as nome_schema_destino,
 nome_artefato_destino   as nome_artefato_destino,
 codigo_comportamento    as codigo_comportamento,
 ordem_prioridade        as ordem_prioridade,
 hora_inicio_execucao    as hora_inicio_execucao
from 
 ${NOME_SCHEMA_DIRO}.artefatos 
where 
 codigo_comportamento = ?
 and ${NOME_SCHEMA_DIRO}.processa_artefato(nome_schema_destino,
                                           nome_artefato_destino,
                                           sysdate,
                                           ?) = 1
 and not exists (select 1
                 from DIRO.execucao_tarefa et
                 where et.codigo_comportamento = ?
                   and to_char(et.data_hora_inicio_execucao, 'YYYY-MM-DD') = to_char(sysdate, 'YYYY-MM-DD')
                   and et.schema_artefato = artefatos.nome_schema_destino
                   and et.nome_artefato   = artefatos.nome_artefato_destino)
 and not exists (select 1
                 from DIRO.execucao_tarefa et
                 where et.codigo_comportamento = (?-1)
                   and to_char(et.data_hora_inicio_execucao, 'YYYY-MM-DD') = to_char(sysdate, 'YYYY-MM-DD')
                   and et.data_hora_fim_execucao is null)
 and ativo = 1
order by 
 artefatos.ordem_prioridade 
</sql>
