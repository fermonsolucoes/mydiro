    <quoteString>N</quoteString>
    <sql>update ${NOME_SCHEMA_DIRO}.execucao
set quantidade_execucao = quantidade_execucao + 1,
    data_hora_ultima_execucao = sysdate
where
 id_execucao = ?;

update ${NOME_SCHEMA_DIRO}.execucao
set data_hora_fim_execucao = sysdate
where 
 codigo_comportamento    > 0
 and data_hora_fim_execucao is null
 and to_char(data_hora_inicio_execucao, 'YYYY-MM-DD') &lt; to_char(sysdate, 'YYYY-MM-DD')
 and ip_computador       = '${var_ip_computador}'
 and nome_computador     = '${var_nome_computador}';

update ${NOME_SCHEMA_DIRO}.comportamento_log
set situacao = 'I',
    data_hora_mudanca_situacao = sysdate
where 
 codigo_comportamento = nvl(?,0)
 and data_hora_fim is null
 and (select count(1)
      from ${NOME_SCHEMA_DIRO}.comportamento_log comp
      where comp.codigo_comportamento = comportamento_log.codigo_comportamento
        and comp.schema_artefato      = comportamento_log.schema_artefato
        and comp.nome_artefato        = comportamento_log.nome_artefato
        and cast(comp.data_hora_inicio as date) = cast(sysdate as date)) = 1
 and exists (select 1
             from ${NOME_SCHEMA_DIRO}.execucao
             where ip_computador   = '${var_ip_computador}'
               and nome_computador = '${var_nome_computador}'
               and id_execucao     = comportamento_log.id_execucao);

update ${NOME_SCHEMA_DIRO}.comportamento_log
set situacao = 'C',
    data_hora_mudanca_situacao = sysdate
where
 id_execucao       = ?
 and data_hora_fim is null
 and situacao      = 'V'
 and exists (select 1
             from ${NOME_SCHEMA_DIRO}.execucao
             where ip_computador   = '${var_ip_computador}'
               and nome_computador = '${var_nome_computador}'
               and id_execucao     = comportamento_log.id_execucao);
</sql>
