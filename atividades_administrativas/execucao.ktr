    <sql>select
 execucao.id_execucao                  as id_execucao,
 execucao.ip_computador                as ip_computador,
 execucao.nome_computador              as nome_computador,
 case when execucao.data_hora_fim_execucao is null
      then 0
      else 1
      end                              as ultima_execucao_finalizou,
 nvl(execucao.codigo_comportamento,0)  as codigo_comportamento,
 data_hora_inicio_execucao             as data_hora_inicio_execucao,
 sysdate                               as data_hora_atual
from
 ${NOME_SCHEMA_DIRO}.execucao
where
 execucao.id_execucao = ${ultimo_id_execucao}

union all

select
 0                    as id_execucao,
 '${ip_computador}'   as ip_computador,
 '${nome_computador}' as nome_computador,
 1                    as ultima_execucao_finalizou,
 null                 as codigo_comportamento,
 sysdate              as data_hora_inicio_execucao,
 sysdate              as data_hora_atual
from
 dual
where
 ${ultimo_id_execucao} = 0

</sql>
