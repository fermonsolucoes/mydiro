where tipo = 'EMB_STRING' 
  and de = '?';

insert into diro.simbolos_caracteres
 (tipo, de, para, ativo)
select 
 'EMB_STRING' as tipo,
 '?'          as de,
 case when '?' = 'A'
      then '§'
      else '?'
      end     as para,
 1            as ativo
from
 dual;
